source func

function another() to integer
begin
	leave 6 + 13
end

function math(integer a, integer b, integer c, integer d)
begin
	put b + c
end

function happy() to integer
begin
	put "here\n";
	leave 5
end

begin
	integer b;
	b:= 6;
	put b."\n";
	put another()."\n";
	put happy()."\n";
	call math(1, 2, 3, 4)
end
